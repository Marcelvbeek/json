{
  "name": "json-rest-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist",
  "scripts": {
    "frontend": "webpack-dev-server --inline",
    "dev": "nodemon -w server --exec \"babel-node server --presets es2015,stage-0\"",
    "build": "babel server -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint server"
  },
  "babel": {
    "presets": [
      "latest",
      "es2015",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "developit/express-es6-rest-api",
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.6.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "html-loader": "^0.4.5",
    "md5": "^2.2.1",
    "morgan": "^1.8.0",
    "ng-redux": "^3.4.0-beta.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.3.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.28.4",
    "es6-template-string": "^1.1.0",
    "eslint": "^3.1.1",
    "file-loader": "^0.11.2",
    "nodemon": "^1.9.2",
    "path": "^0.12.7",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.5.0"
  }
}
